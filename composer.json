{
    "name": "barryvdh/laravel-form-bridge",
    "description": "This packages integrates Symfony Form Component in Laravel.",
    "keywords": ["laravel", "form", "formbuilder", "forms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x|6.0.x",
        "symfony/form": "^3.4|^4",
        "symfony/validator": "^3|^4",
        "symfony/twig-bridge": "^3|^4",
        "twig/twig": "~1.30|~2.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\Form\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.3",
        "orchestra/testbench": "3.x",
        "phpunit/phpunit": "^4.8|^5.2"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
